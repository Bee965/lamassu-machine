{
	"exchanges": {
    "plugins": {
      "available": {
        "ticker": ["bitpay", "mtgox", "bitstamp", "custom_ticker"],
        "trade": ["bitstamp"],
        "transfer": ["blockchain"]        
      },
      "info": {
        "custom_ticker": {
          "display": "Custom Price Feed",
          "fields": [{"code": "uri", "display": "Feed URL"}]
        },
        "bitpay": {"display": "BitPay"},
        "mtgox": {"display": "MtGox"},
        "bitstamp": {"display": "Bitstamp"},
        "blockchain": {
          "display": "Blockchain.info",
          "fields": [
            {"code": "guid", "display": "User ID"},
            {"code": "password", "display": "Password"},
            {"code": "fromAddress", "display": "Bitcoin Address"}
          ]
        },
        "bitstamp_trade": {
          "display": "Bitstamp",
          "fields": [
            {"code": "clientId", "display": "Customer ID"},
            {"code": "key", "display": "API Key"},
            {"code": "secret", "display": "Secret"}
          ]
        }
      }
    }
  }
}
