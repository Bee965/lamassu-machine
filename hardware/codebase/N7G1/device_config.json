{
  "brain": {
    "freeMemRatio": 0.01,
    "powerStatus": null,
    "dataPath": "/var/lib/sencha",
    "powerStatus": "/sys/class/power_supply/battery/status",
    "batteryCapacityPath": "/sys/class/power_supply/battery/capacity"
  },
  "wifi": {
    "wpa": {
      "socket": "/tmp/wpa_supplicant"
    },
    "wpaConfigPath": "wpa.conf"
  },
  "id003": {
    "rs232": {
      "device": "/dev/ttyUSB0"
    }
  },
  "scanner": {
    "device": "/dev/video0",
    "width": 1280,
    "height": 720,
    "contrast": 25,
    "lowExposure": 15,
    "highExposure": 50,
    "exposureAdjustTime": 200
  },
  "updater": {
    "caFile": "/usr/local/share/sencha/certs/lamassu.pem",
    "downloadDir": "/tmp/download",
    "extractDir": "/tmp/extract",
    "extractor": {
      "lamassuPubKeyFile": "/usr/local/share/sencha/pubkeys/lamassu.pub.key"
    }
  }
}
